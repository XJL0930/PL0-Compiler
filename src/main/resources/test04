PROGRAM nestedTest
CONST x := 5, y := 10, z := 15;
VAR a, b, c;
BEGIN
    a := x;
    b := y;
    c := z;

    IF a < b THEN
        WHILE c > x DO
        BEGIN
            a := a + 1;
            IF a > c THEN
                b := b * 2;
            ;
            IF a < c THEN
                b := b / 2;
        END
END
